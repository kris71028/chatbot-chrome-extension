{
    "manifest_version": 3,
    "name": "Chatbot Extension",
    "version": "1.0",
    "description": "A simple chatbot extension.",
    "action": {
      "default_popup": "popup.html",
      "default_icon": "icon.png"
      
    },
    "permissions": [
      "activeTab"
    ],
    "icons": {
      "16": "icon-16.png",
      "48": "icon-48.png",
      "128": "icon-128.png"
    },
    "background": {
      "service_worker": "background.js"
    },
    "host_permissions": [
      "*://*/*"
    ]

  }
